	"tab_button": {
		"name": "main_panel",
		"layout": {
			"width": "auto",
			"height": "100%"
		},
		"visuals": {
			"geometries": [
				{
					"type": "rectangle",
					"top_left": [
						[ 0.0, 1.0 ],
						[ 0.0, 0.0 ]
					],
					"bottom_right": [
						[ 1.0, 1.0 ],
						[ 0.0, 0.0 ]
					]
				},
				{
					"type": "rectangle",
					"top_left": [
						[ 0.5, 0.0 ],
						[ 0.0, 0.0 ]
					],
					"bottom_right": [
						[ 0.5, 0.0 ],
						[ 0.0, 2.0 ]
					]
				},
				{
					"type": "rectangle",
					"top_left": [
						[ 0.0, 0.0 ],
						[ 0.0, 0.0 ]
					],
					"bottom_right": [
						[ 1.0, 1.0 ],
						[ 0.0, 0.0 ]
					],
					"fill": {
						"type": "solid",
						"color": [ 1.0, 1.0, 1.0, 0.0 ]
					}
				}
			]
		},
		"animations": {
			"mouse_enter": {
				"visuals.geometries[2].fill.(solid_color_brush.color).a": {
					"to": 0.2,
					"duration": 0.1,
					"transition": "linear"
				}
			},
			"mouse_leave": {
				"visuals.geometries[2].fill.(solid_color_brush.color).a": {
					"to": 0.0,
					"duration": 0.1,
					"transition": "linear"
				}
			}
		},
		"children": [
			{
				"type": "stack_panel",
				"class": "tab_button_background",
				"layout": {
					"height": "100%",
					"padding": 5.0
				},
				"children": [
					{
						"type": "label",
						"name": "label",
						"visibility": "vl",
						"layout": {
							"margin": [ 0.0, 1.0, 0.0, 1.0 ],
							"anchor": "lr"
						}
					},
					{
						"type": "button",
						"class": "tab_button.close_button",
						"name": "close_button",
						"visuals": {
							"geometries": [
								{
									"type": "path",
									"subpaths": [
										[
											[
												[ 0.2, 0.2 ],
												[ 0.0, 0.0 ]
											],
											{
												"line_to": [
													[ 0.8, 0.8 ],
													[ 0.0, 0.0 ]
												]
											},
											{ "closed": false }
										],
										[
											[
												[ 0.8, 0.2 ],
												[ 0.0, 0.0 ]
											],
											{
												"line_to": [
													[ 0.2, 0.8 ],
													[ 0.0, 0.0 ]
												]
											},
											{ "closed": false }
										]
									],
									"stroke": {
										"type": "solid",
										"color": [ 0.0, 0.0, 0.0, 0.0 ],
										"thickness": 1.5
									}
								}
							]
						},
						"animations": {
							"mouse_enter": {
								"visuals.geometries[0].stroke.(solid_color_brush.color).r": {
									"to": 0.8,
									"duration": 0.1,
									"transition": "linear"
								}
							},
							"mouse_leave": {
								"visuals.geometries[0].stroke.(solid_color_brush.color).r": {
									"to": 0.0,
									"duration": 0.1,
									"transition": "linear"
								}
							},

							"main_panel.mouse_enter": {
								"visuals.geometries[0].stroke.(solid_color_brush.color).a": {
									"to": 1.0,
									"duration": 0.1,
									"transition": "linear"
								}
							},
							"main_panel.mouse_leave": {
								"visuals.geometries[0].stroke.(solid_color_brush.color).a": {
									"to": 0.0,
									"duration": 0.1,
									"transition": "linear"
								}
							}
						},
						"layout": {
							"width": "15px",
							"height": "15px",
							"margin": [ 5.0, 1.0, 0.0, 1.0 ],
							"anchor": "lr"
						}
					}
				]
			}
		]
	},
